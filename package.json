{
  "name": "goody2shoes-matcha-brochure",
  "version": "1.0.0",
  "description": "Vibrant webapp brochure for Goody2Shoes Matcha Pop-Up Jakarta",
  "main": "index.html",
  "scripts": {
    "build": "node -e \"console.log('Validating static site files...'); const fs = require('fs'); const files = ['index.html', 'styles.css', 'script.js', 'netlify.toml']; files.forEach(file => { try { fs.accessSync(file); console.log('✓ ' + file); } catch(e) { console.error('✗ Missing: ' + file); process.exit(1); } }); console.log('\\n✓ Build validation successful! All required files present.');\",
    "dev": "python3 -m http.server 8888",
    "start": "python3 -m http.server 8888",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "matcha",
    "jakarta",
    "cafe",
    "brochure",
    "webapp",
    "static-site"
  ],
  "author": "Goody2Shoes Matcha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/billsusanto/Goody2Shoes-Brochure"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
